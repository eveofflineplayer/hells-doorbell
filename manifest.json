{
  "manifest_version": 3,
  "name": "Hells doorbell",
  "version": "1.33.7",
  "description": "Pathfinder addon",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": [
    "webRequest",
    "activeTab",
    "scripting"
  ],
  "web_accessible_resources": [
    {
      "resources": ["ding.wav"],
      "matches": ["https://pf.darkventure.space/*"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["https://pf.darkventure.space/*","https://zkillboard.com/character/*"],
      "js": ["apex.js", "content.js"],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; connect-src ws://localhost:* wss://* http://localhost:*"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
